<!-- prayers.njk -->
{% set title = "Normal To Noble" %}
{% set id = "normal-to-noble" %}

{% extends "layout.njk" %}

{% block content %}
  <!-- Adds the nav macro -->
  {% import 'macros/nav.njk' as nav %}
  {{ nav.build({ icon: 'fad fa-chess-knight', title: title, id: id }) }}

  <!--- Content for this page -->
  <main>
    {% import 'macros/tabs.njk' as tabs %}
    {{ tabs.build({
    tabfile: id,
    tabs: [{
      icon: 'fad fa-fire',
      title: 'Normal'
    }, {
      icon: 'fad fa-chess-knight',
      title: 'Noble'
    }]
  }) }}
  </main>

{% endblock %}
